{
  "name": "progetto_ai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"npx tsx src/index.ts\"",
    "lint": "npx eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma db push --force-reset",
    "db:seed": "node dist/seed.js",
    "setup": "npm install && npm run db:push && npm run build && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@types/nodemailer": "^7.0.3",
    "eslint": "^9.38.0",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "inquirer": "^12.10.0",
    "nodemailer": "^7.0.10",
    "nodemon": "^3.1.10"
  }
}
